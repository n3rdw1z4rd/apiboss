<div class="tab-pane" id="accounts">
	<h3>Accounts</h3>
	<table class="table table-striped table-bordered table-hover">
		<thead>
			<tr>
				<th>Email</th>
				<th>Status</th>
				<th>Role</th>
				<th>Subscription</th>
			</tr>
		</thead>
		<tbody>
			<% accountList.forEach(account => { %>
				<tr data-action="/admin/editAccount" data-id="<%= account.id %>">
					<th scope="row"><%= account.email %></th>
					<td><%= constants.ACCOUNT_STATUS.getName(account.status) %></td>
					<td><%= constants.ACCOUNT_ROLE.getName(account.role) %></td>
					<td><%= constants.ACCOUNT_SUBSCRIPTION.getName(account.subscription) %></td>
				</tr>
			<% }) %>
		</tbody>
	</table>
</div>
