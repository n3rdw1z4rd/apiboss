<div class="modal fade" id="create-api-modal" role="dialog" tabindex="-1">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal">&times;</button>
				<h3 class="modal-title text-center">Create New API</h3>
			</div>
			<div class="modal-body">
				<form method="post" action="/api/createApi">
					<p><input id="name" class="form-control" name="name" type="text" placeholder="Name" required autofocus/></p>
					<p><button class="form-control btn btn-primary" type="submit">Create</button></p>
				</form>
			</div>
			<div class="modal-footer"></div>
		</div>
	</div>
</div>
